{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "dbHeroku",
    "options": {
      "realmRequired": true,
      "realmDelimiter": ":"
    }
  },
  "AccessToken": {
    "dataSource": "dbHeroku",
    "public": false,
    "relations": {
      "people": {
        "type": "belongsTo",
        "model": "People",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "dbHeroku",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "dbHeroku",
    "public": true,
    "options": {
      "strictObjectIDCoercion": false
    }
  },
  "Role": {
    "dataSource": "dbHeroku",
    "public": false
  },
  "People": {
    "dataSource": "dbHeroku",
    "public": true
  },
  "Categories": {
    "dataSource": "dbHeroku",
    "public": true
  },
  "Container": {
    "dataSource": "FileStorage",
    "public": true
  },
  "Email": {
    "dataSource": "Email"
  },
  "Menu": {
    "dataSource": "dbHeroku",
    "public": true
  },
  "Wallet": {
    "dataSource": "dbHeroku",
    "public": true
  },
  "Transaction": {
    "dataSource": "dbHeroku",
    "public": true
  },
  "MealType": {
    "dataSource": "dbHeroku",
    "public": true
  },
  "Order": {
    "dataSource": "dbHeroku",
    "public": true
  }
}
